class parrotTrouble
{
	public boolean parrot(boolean talking,int hour)
	{
		return talking &&(hour<7||hour>20);
	}
	public static void main(String[]args)
	{
		parrotTrouble pt=new parrotTrouble();
		System.out.println(ptparrotTrouble(true,6));
		System.out.println(ptparrotTrouble(true,7));
		System.out.println(ptparrotTrouble(false,6));
		System.out.println(ptparrotTrouble(true,21));
	}
}
